{
	"name": "LS_REST_CHINA_FIIX",
	"properties": {
		"description": "Extracts data from Fiix system via API calls",
		"parameters": {
			"KeyVaultBaseUrl": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "@{linkedService().KeyVaultBaseUrl}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous",
			"authHeaders": {
				"Authorization": {
					"type": "AzureKeyVaultSecret",
					"store": {
						"referenceName": "LS_KeyVault",
						"type": "LinkedServiceReference"
					},
					"secretName": {
						"value": "khcde-kv-sec-fiix-china-auth-token",
						"type": "Expression"
					}
				}
			}
		}
	}
}