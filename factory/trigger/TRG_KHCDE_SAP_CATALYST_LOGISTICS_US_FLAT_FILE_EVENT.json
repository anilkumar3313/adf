{
	"name": "TRG_KHCDE_SAP_CATALYST_LOGISTICS_US_FLAT_FILE_EVENT",
	"properties": {
		"description": "Triggers the orchestration that processes flat files for all us/canada logistics project. Also triggers full pallet kpi execution for WHS US.",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "PL_KHCDE_MANUFACTURING_LOGISTICS_POOL_LOAD",
					"type": "PipelineReference"
				},
				"parameters": {
					"ApplicationName": "transportation",
					"adf_trigger_name": "@trigger().outputs.body.fileName"
				}
			},
			{
				"pipelineReference": {
					"referenceName": "PL_KHCDE_ORDER_MANAGEMENT_DBT_LOAD",
					"type": "PipelineReference"
				},
				"parameters": {
					"ApplicationName": "order_management",
					"DatasetGroup": "@trigger().outputs.body.fileName"
				}
			}
		],
		"type": "BlobEventsTrigger",
		"typeProperties": {
			"blobPathBeginsWith": "/khc-enterprise-data/blobs/logistics/transportation/flat_files/triggers/",
			"blobPathEndsWith": ".txt",
			"ignoreEmptyBlobs": true,
			"scope": "/subscriptions/6bb7b46f-5fed-48cc-91c6-caac569de97d/resourceGroups/KHCDE-E2-RG-DEV/providers/Microsoft.Storage/storageAccounts/khcdedatastoragedev",
			"events": [
				"Microsoft.Storage.BlobCreated"
			]
		}
	}
}