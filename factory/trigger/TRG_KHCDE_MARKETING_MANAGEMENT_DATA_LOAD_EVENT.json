{
	"name": "TRG_KHCDE_MARKETING_MANAGEMENT_DATA_LOAD_EVENT",
	"properties": {
		"description": "Its an Event based trigger which loads the Marketing Management Data",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "PL_KHCDE_MARKETING_MANAGEMENT_MASTER_LOAD",
					"type": "PipelineReference"
				},
				"parameters": {
					"application_name": "marketing_management",
					"dataset_name": "@replace(trigger().outputs.body.fileName,'.txt','')",
					"Trigger_file_name": "@trigger().outputs.body.fileName"
				}
			}
		],
		"type": "BlobEventsTrigger",
		"typeProperties": {
			"blobPathBeginsWith": "/khc-enterprise-data/blobs/marketing/marketing_management/triggers/marketing_management_",
			"blobPathEndsWith": ".txt",
			"ignoreEmptyBlobs": true,
			"scope": "/subscriptions/6bb7b46f-5fed-48cc-91c6-caac569de97d/resourceGroups/KHCDE-E2-RG-DEV/providers/Microsoft.Storage/storageAccounts/khcdedatastoragedev",
			"events": [
				"Microsoft.Storage.BlobCreated"
			]
		}
	}
}