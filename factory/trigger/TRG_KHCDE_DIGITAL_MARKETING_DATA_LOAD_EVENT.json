{
	"name": "TRG_KHCDE_DIGITAL_MARKETING_DATA_LOAD_EVENT",
	"properties": {
		"description": "Its an Event based trigger which loads the Digital Marketing Data",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "PL_KHCDE_DIGITAL_MRKT_MASTER_LOAD",
					"type": "PipelineReference"
				},
				"parameters": {
					"application_name": "digital_marketing",
