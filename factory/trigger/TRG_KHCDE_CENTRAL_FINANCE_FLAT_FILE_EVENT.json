{
	"name": "TRG_KHCDE_CENTRAL_FINANCE_FLAT_FILE_EVENT",
	"properties": {
		"description": "Adhoc trigger that triggers when ever the file from SharePoint comes to the desired folder",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "PL_KHCDE_LATAM_FINANCE_MASTER_POOL_LOAD",
					"type": "PipelineReference"
				},
				"parameters": {
					"ApplicationName": "central_finance",
					"adf_trigger_name": "@trigger().outputs.body.fileName"
				}
			}
		],
		"type": "BlobEventsTrigger",
		"typeProperties": {
			"blobPathBeginsWith": "/khc-enterprise-data/blobs/central_finance/waste_ytd/trigger/",
			"blobPathEndsWith": ".txt",
			"ignoreEmptyBlobs": true,
			"scope": "/subscriptions/6bb7b46f-5fed-48cc-91c6-caac569de97d/resourceGroups/KHCDE-E2-RG-DEV/providers/Microsoft.Storage/storageAccounts/khcdedatastoragedev",
			"events": [
				"Microsoft.Storage.BlobCreated"
			]
		}
	}
}