{
	"name": "TRG_KHCDE_RESEARCH_AND_DEVELOPMENT_STORAGE_BLOB_EVENT",
	"properties": {
		"description": "Triggers pipeline based on blob trigger file in khcdedatastorage<env>",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "PL_KHCDE_RESEARCH_AND_DEVELOPMENT_MASTER_LOAD",
					"type": "PipelineReference"
				},
				"parameters": {
					"ApplicationName": "research_and_development",
					"BlobTriggerName": "@trigger().outputs.body.fileName"
				}
			}
		],
		"type": "BlobEventsTrigger",
		"typeProperties": {
			"blobPathBeginsWith": "/khc-enterprise-data/blobs/research_and_development/triggers",
			"blobPathEndsWith": ".txt",
			"ignoreEmptyBlobs": true,
			"scope": "/subscriptions/6bb7b46f-5fed-48cc-91c6-caac569de97d/resourceGroups/KHCDE-E2-RG-DEV/providers/Microsoft.Storage/storageAccounts/khcdedatastoragedev",
