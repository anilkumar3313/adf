{
	"name": "TRG_KHCDE_BRACKET_PRICING_EVENT",
	"properties": {
		"description": "This trigger is for loading the bracket pricing flat files as they are uploaded to the sharepoint  location",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "PL_KHCDE_ACCOUNT_RECEIVABLES_MASTER_POOL_LOAD",
					"type": "PipelineReference"
				},
				"parameters": {
					"ApplicationName": "order_management",
					"adf_trigger_name": "@trigger().outputs.body.fileName",
					"email_notify_ind": "n"
				}
			}
		],
		"type": "BlobEventsTrigger",
		"typeProperties": {
			"blobPathBeginsWith": "/khc-enterprise-data/blobs/logistics/order_management/bracket_pricing_triggers/",
			"blobPathEndsWith": ".txt",
			"ignoreEmptyBlobs": true,
			"scope": "/subscriptions/6bb7b46f-5fed-48cc-91c6-caac569de97d/resourceGroups/KHCDE-E2-RG-DEV/providers/Microsoft.Storage/storageAccounts/khcdedatastoragedev",
			"events": [
				"Microsoft.Storage.BlobCreated"
			]
		}
	}
}