					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "select * from khc_orchestration_metadata where application_name = '@{pipeline().parameters.ApplicationName}' and dataset_group = '@{concat(split(pipeline().parameters.BlobTrigger,'_')[1],'_',split(pipeline().parameters.BlobTrigger,'_')[2])}'  and active_ind = 'y'",
