					{
						"activity": "LU_EXTRACT_METADATA",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('LU_EXTRACT_METADATA').output.value",
						"type": "Expression"
					},
					"isSequential": false,
