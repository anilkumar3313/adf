					}
				},
				"linkedServiceName": {
					"referenceName": "LS_SQL",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"application_name": {
				"type": "string"
			},
			"adf_trigger_name": {
				"type": "string"
			}
		},
		"folder": {
