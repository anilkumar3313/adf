{
	"name": "PL_KHCDE_SUB_APAC_DEMANTRA_DF_ORCL_EXTRACT",
	"properties": {
		"description": "Sub pipeline for extract data from DB2 DWH DB for  Sales Open Order table",
		"activities": [
			{
				"name": "Demantra_Server_Check",
				"description": "Check the Status for Demantra Table",
				"type": "Until",
				"dependsOn": [
					{
						"activity": "SP-PipelineStart",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@equals(variables('exit_ind'), 'Y')",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "LU-ORC_Demantra_Metadata",
							"description": "Look up for control table",
							"type": "Lookup",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "OracleSource",
									"oracleReaderQuery": {
										"value": "SELECT \nCASE WHEN \nNVL(MAX(LAST_UPDATE_DATE),'31-DEC-1900') > \nNVL(MAX(LAST_PROCESSED_DATE),'31-DEC-1900')\nTHEN 'Y' ELSE 'N' END as available\nFROM SNOW.BIEO_AUS_NZ_DEM_TPM_REP\nhaving count(*) > 1",
										"type": "Expression"
									},
									"partitionOption": "None",
									"queryTimeout": "02:00:00"
								},
								"dataset": {
									"referenceName": "DS_KHCDE_APAC_INDO_SEABPP_ORC",
									"type": "DatasetReference",
									"parameters": {
										"KeyVaultSecretName": {
											"value": "@pipeline().parameters.key_vault_secret_name",
											"type": "Expression"
										}
									}
								},
								"firstRowOnly": true
							}
						},
						{
							"name": "Set variable",
							"description": "Set variable value",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "LU-ORC_Demantra_Metadata",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "exit_ind",
								"value": {
									"value": "@{activity('LU-ORC_Demantra_Metadata').output.firstRow.available}",
									"type": "Expression"
								}
							}
						},
						{
							"name": "Wait",
							"description": "Wait for 10 min",
							"type": "Wait",
							"dependsOn": [
								{
									"activity": "LU-ORC_Demantra_Metadata",
									"dependencyConditions": [
										"Failed"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"waitTimeInSeconds": {
									"value": "@int(600)",
									"type": "Expression"
								}
							}
						}
					],
					"timeout": "0.01:00:00"
				}
			},
			{
				"name": "SP-PipelineStart",
				"description": "This Activity logs starting of pipeline in khc_audit_logÂ table.",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 5,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[dbo].[generic_pipeline_audit_log]",
					"storedProcedureParameters": {
						"action_ind": {
							"value": {
								"value": "I",
								"type": "Expression"
							},
							"type": "String"
						},
						"application_name": {
							"value": {
								"value": "apac_anz_df_master_data",
								"type": "Expression"
							},
							"type": "String"
						},
						"dataset_name": {
							"value": "SalesOpenOrders",
							"type": "String"
						},
						"pipeline_name": {
							"value": {
								"value": "@pipeline().Pipeline",
								"type": "Expression"
							},
							"type": "String"
						},
						"pipeline_run_id": {
							"value": {
								"value": "@pipeline().RunId",
								"type": "Expression"
							},
							"type": "String"
						},
						"pipeline_status": {
							"value": {
								"value": "Extract Pipeline Started.",
								"type": "Expression"
							},
							"type": "String"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "LS_SQL",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "SP-Pipelinesuccesful",
