{
	"name": "PL_KHCDE_JDA_TO_OMP_FORECAST_DATA_LOAD",
	"properties": {
		"description": "To extract forecast data from the JDA Server and load into SFK and from SFK to OMP Blob",
		"activities": [
			{
				"name": "LU-JDA_Forecast_Metadata",
				"description": "Look-up activity to get metadata for JDA forecast data.",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "SP-PipelineStart_Extract_intial",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 5,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "select om.*\nfrom khc_orchestration_metadata om inner join dbo.khc_data_extract_control_table ctrl\non om.parent_id = ctrl.parent_id\nwhere om.application_name in ('omp')\nand om.active_ind = 'y' and data_level=2\nand om.dataset_group = 'omp_jda_forecast'",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "DS_KHCDE_GENERIC_MSSQL_Metadata",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "ForEach-JDA_Inp",
				"description": "ForEach activity to extract forecast data from JDA server to KHC staging area.",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "LU-JDA_Forecast_Metadata",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('LU-JDA_Forecast_Metadata').output.value",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "CP-ForecastFile_Loadto_KHC",
							"description": "Forecast data to be extracted from Oracle JDA Server to BLOB",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 5,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "OracleSource",
									"partitionOption": "None",
									"queryTimeout": "02:00:00"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "DS_KHCDE_OMP_JDA_SOURCE",
									"type": "DatasetReference",
									"parameters": {
										"TableName": {
											"value": "@{toUpper(item().data_filename)}",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DS_KHCDE_OMP_BLOB_FOLDER_FILE",
									"type": "DatasetReference",
									"parameters": {
										"file": {
											"value": "@concat(item().data_filename, '.tsv')",
											"type": "Expression"
										},
										"blobpath": {
											"value": "@{item().stage_blob_name}",
											"type": "Expression"
										},
										"container": {
											"value": "@{item().container_name}",
											"type": "Expression"
										}
									}
								}
							]
						},
