{
	"name": "TRG_KHCDE_APAC_SALES_BLOB",
	"properties": {
		"description": "Trigger all entities in sales_management based on blob_trigger_name",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "PL_KHCDE_APAC_MASTER_POOL_LOAD",
					"type": "PipelineReference"
				},
				"parameters": {
					"ApplicationName": "sales_management_international','international_unified_view','khc_logistics','production_management','supply_chain_finance','khc_manufacturing','khc_procurement",
					"BlobTriggerName": "@trigger().outputs.body.fileName"
