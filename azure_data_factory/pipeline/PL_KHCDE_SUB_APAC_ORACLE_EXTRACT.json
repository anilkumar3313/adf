					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "select\n    om.application_name,\n    om.dataset_name,\n    ctrl.table_name,\n    om.server_name,\n    om.server_path as source_object_type,\n    om.filename as dso_changelog_table,\n    om.data_filename,\n    om.container_name,\n    om.archival_blob_name,\n    om.stage_blob_name,\n    om.blob_trigger_name,\n    om.data_level,\n    om.active_ind,\n    om.parent_id,\n    ctrl.dynamic_partition_ind,\n    ctrl.partition_column,\n    ctrl.incr_column_name,\n    ctrl.last_incr_value,\n    case\n      when server_name = 'LS_APAC_INDO_ORCL' then 'khcde-kv-sec-indo-seabpp-khsnflk-oracle-conn-string'\n      when server_name = 'LS_APAC_DWH_ORCL' then 'khcde-kv-sec-indo-seabpp-dwh-oracle-conn-string'\n      when server_name = 'LS_APAC_LCA_ORCL' then 'khcde-kv-sec-indo-seabpp-khsnflk-lca-oracle-conn-string'\n      when server_name = 'LS_TPM_DEMANTRA_ORCL' then 'khcde-kv-sec-tpm-demantra-oracle-conn-string'\n     when server_name = 'LS_APAC_ANZ_ORCL' then 'khcde-kv-sec-demantra-oracle-conn-string'\n  when server_name = 'LS_APAC_ANZ_JDA_ORACLE' then 'khcde-kv-sec-apac-anz-oracle-conn-string'\nEND AS key_vault_secret_name\nfrom dbo.khc_orchestration_metadata om inner join\n( Select\n    parent_id,\n    table_name,\n    dynamic_partition_ind,\n    partition_column,\n    where_clause,\n    incr_column_name,\n    last_incr_value\nfrom dbo.khc_data_extract_control_table\n) ctrl\non om.parent_id = ctrl.parent_id\nwhere len(trim(om.server_name)) > 0\n    and om.application_name in ('@{pipeline().parameters.application_name}')\n  and om.adf_trigger_name in ('@{pipeline().parameters.adf_trigger_name}')\n  and om.server_name IN ('LS_APAC_INDO_ORCL','LS_APAC_DWH_ORCL','LS_APAC_LCA_ORCL','LS_TPM_DEMANTRA_ORCL','LS_APAC_ANZ_ORCL','LS_APAC_ANZ_JDA_ORACLE')\n  and om.data_level=2 and om.active_ind='y'",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "DS_KHCDE_GENERIC_MSSQL_Metadata",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
