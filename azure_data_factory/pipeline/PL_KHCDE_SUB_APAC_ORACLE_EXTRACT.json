		"activities": [
			{
				"name": "LU-ORC_Metadata",
				"description": "This LU Metadata activity fetches required details for data extraction from orchestration and dataengg_extract table ",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "SP-PipelineStart",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
