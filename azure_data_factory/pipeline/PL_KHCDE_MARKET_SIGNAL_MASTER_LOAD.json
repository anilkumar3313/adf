				"description": "Calculates the date from which data is to be processed.",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Pipeline_Start",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "date_to_process_from",
					"value": {
						"value": "@startOfDay(getPastTime(pipeline().parameters.days_to_process,'Day'))",
						"type": "Expression"
					}
				}
			},
