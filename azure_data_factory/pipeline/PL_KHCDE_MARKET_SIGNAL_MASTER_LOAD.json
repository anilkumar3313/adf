						"referenceName": "DS_KHCDE_GENERIC_BLOB_BINARY_Folder",
						"type": "DatasetReference",
						"parameters": {
							"container_name": {
								"value": "@activity('Fetch_Metadata').output.value[0].container_name",
								"type": "Expression"
							},
							"blob_name": {
