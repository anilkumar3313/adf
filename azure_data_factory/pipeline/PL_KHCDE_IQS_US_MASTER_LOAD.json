								"pipeline": {
									"referenceName": "PL_KHCDE_IQS_US_EXTRACT_LOAD",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"ApplicationName": {
										"value": "@item().application_name",
										"type": "Expression"
									},
									"Plant_name": {
										"value": "@item().Plant_name",
										"type": "Expression"
									},
									"DatasetGroup": {
