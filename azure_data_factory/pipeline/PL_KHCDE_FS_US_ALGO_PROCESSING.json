					{
						"referenceName": "DS_KHCDE_GENERIC_BLOB_CSV_FolderFile",
						"type": "DatasetReference",
						"parameters": {
							"container_name": "khc-enterprise-data",
							"blob_name": "sales/foodservices/us/triggers/",
							"file_name": {
								"value": "@activity('Fetch_Orch_Metadata_Data').output.value[0].dbt_trigger_name",
