											"dataset": {
												"referenceName": "DS_KHCDE_GENERIC_KHC_DATASHARE_STRGV2_Folder",
												"type": "DatasetReference",
												"parameters": {
													"container_name": {
														"value": "@if(equals(item().sftp_copy_ind,'y'),substring(item().sftp_path,0,indexof(item().sftp_path,'/')),item().container_name)",
														"type": "Expression"
													},
													"blob_name": {
														"value": "@if(equals(item().sftp_copy_ind,'y'),substring(item().sftp_path,add(indexof(item().sftp_path,'/'),1),sub(sub(length(item().sftp_path),indexof(item().sftp_path,'/')),1)),item().extract_blob_location)",
														"type": "Expression"
													}
												}
											},
											"enableLogging": false,
											"storeSettings": {
												"type": "AzureBlobFSReadSettings",
												"recursive": false
											}
										}
									}
								]
							}
						},
						{
							"name": "GM-StageExistence",
							"description": "Get metadata of extract stage blob",
							"type": "GetMetadata",
							"dependsOn": [
								{
									"activity": "IF-SharedExtractBlobExists",
									"dependencyConditions": [
										"Succeeded"
