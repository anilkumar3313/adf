				"typeProperties": {
					"items": {
						"value": "@activity('LU-SFK_Extraction_Metadata').output.value",
						"type": "Expression"
					},
					"isSequential": false,
					"activities": [
						{
							"name": "GM-SharedExtractBlobExistence",
							"description": "Get metadata for blob in khcdedatashare<env> storage account",
							"type": "GetMetadata",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataset": {
									"referenceName": "DS_KHCDE_GENERIC_KHC_DATASHARE_STRGV2_Folder",
									"type": "DatasetReference",
									"parameters": {
										"container_name": {
											"value": "@if(equals(item().sftp_copy_ind,'y'),substring(item().sftp_path,0,indexof(item().sftp_path,'/')),item().container_name)",
											"type": "Expression"
										},
										"blob_name": {
											"value": "@if(equals(item().sftp_copy_ind,'y'),substring(item().sftp_path,add(indexof(item().sftp_path,'/'),1),sub(sub(length(item().sftp_path),indexof(item().sftp_path,'/')),1)),item().extract_blob_location)",
											"type": "Expression"
										}
									}
								},
								"fieldList": [
									"exists"
								],
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true
								},
								"formatSettings": {
									"type": "BinaryReadSettings"
								}
							}
						},
						{
							"name": "IF-SharedExtractBlobExists",
							"description": "If blob is present in shared storage account delete the old data",
							"type": "IfCondition",
							"dependsOn": [
								{
									"activity": "GM-SharedExtractBlobExistence",
									"dependencyConditions": [
										"Succeeded"
