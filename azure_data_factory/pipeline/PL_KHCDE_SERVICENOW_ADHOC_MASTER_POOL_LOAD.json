			{
				"name": "LU-AuditFail",
				"description": "This activity loops through the activity in the audit table for tables which have failed.",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "EP-ServiceNow",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
