					"timeout": "7.00:00:00",
					"retry": 5,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "select\n o1.application_name\n,o1.dataset_name\n,o1.data_filename\n,o1.container_name\n,o1.stage_blob_name\nfrom khc_orchestration_metadata o1\n\t,(select\n\t  dataset_name\n\t ,max(last_extract_date) as last_extract_date\n\t from khc_api_data_extract_audit_log \n\t where status = 'fail'\nand application_name = '@{pipeline().parameters.application_name}'\nand dataset_name = '@{pipeline().parameters.dataset_name}'\n\t group by dataset_name) o2\n\t,(select\n\t  dataset_name\n\t ,max(last_extract_date) as last_extract_date\n\t from khc_api_data_extract_audit_log\n         where dataset_name = '@{pipeline().parameters.dataset_name}'\nand application_name = '@{pipeline().parameters.application_name}'\n\t group by dataset_name) o3\nwhere o1.dataset_name\t\t= o2.dataset_name\n  and o2.last_extract_date >= o3.last_extract_date\n  and o1.dataset_name\t\t= o3.dataset_name\nand  o1.dataset_name = '@{pipeline().parameters.dataset_name}'\nand o1.application_name = '@{pipeline().parameters.application_name}'",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "DS_KHCDE_GENERIC_MSSQL_Metadata",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
