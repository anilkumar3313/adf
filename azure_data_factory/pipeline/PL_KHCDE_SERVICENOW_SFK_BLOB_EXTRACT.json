						"sqlReaderQuery": {
							"value": "select * from khc_orchestration_metadata where\napplication_name = '@{pipeline().parameters.application_name}'\nand server_name = 'service_now_log_data'\nand active_ind='y'",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
